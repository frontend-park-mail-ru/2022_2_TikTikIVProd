<div class="menu-container">
    <div class="menu__title">
        {{title}}
    </div>
    <div class="menu__items__container">
        {{#each items as |item|}}
        <div href="{{item.href}}" class="menu__item {{#if item.style }} {{item.style}} {{/if}}">
            {{item.text}}
        </div>
        {{/each}}
    </div>
</div>